{
    "manifest_version": 2,
    "name": "ygg_ratio",
    "version": "1.1.1",

    "description": "Adds the ratio between seed and leech on all tables for the yggtorrent.com site",

    "icons": {
        "48": "icons/yggratio-48.png"
    },

    "content_scripts": [
      {
          "matches": ["*://*.yggtorrent.com/*"],
          "js": ["ratio.js"]
      }
    ]
}
